{
   "name": "mattbotreborn",
   "version": "1.0.1",
   "description": "A multipurpose Discord bot written using Typescript.",
   "repository": "https://github.com/matthieu-locussol/MattBotReborn",
   "author": "Matthieu Locussol",
   "license": "MIT",
   "scripts": {
      "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/main.ts",
      "build": "tsc",
      "start": "cross-env NODE_ENV=production node build/main.js",
      "check:prettier": "prettier --list-different --config ./.prettierrc.json --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json}\"",
      "check:eslint": "eslint --ignore-path .gitignore .",
      "check:typescript": "tsc --project ./tsconfig.json --noEmit"
   },
   "pre-commit": [
      "check:typescript",
      "check:eslint",
      "check:prettier"
   ],
   "dependencies": {
      "@discordjs/opus": "0.5.0",
      "axios": "0.21.1",
      "bufferutil": "^4.0.3",
      "canvas": "^2.8.0",
      "chart.js": "2.9.4",
      "chartjs-node-canvas": "3.0.6",
      "child-process-promise": "2.2.1",
      "discord.js": "git+https://github.com/discordjs/discord.js.git#f7e420bf290f5404e8b99b8896c63e178f77bbfc",
      "erlpack": "discord/erlpack",
      "ffmpeg-static": "4.3.0",
      "i18next": "20.3.0",
      "keyv": "4.0.3",
      "keyv-file": "0.2.0",
      "libsodium-wrappers": "^0.7.9",
      "merge-images": "^2.0.0",
      "moment": "2.29.1",
      "node-osu": "2.2.1",
      "ojsama": "2.2.0",
      "utf-8-validate": "^5.0.5",
      "winston": "3.3.3",
      "ytdl-core-discord": "^1.3.1",
      "zlib-sync": "^0.1.7"
   },
   "devDependencies": {
      "@types/chart.js": "2.9.32",
      "@types/child-process-promise": "2.2.1",
      "@types/keyv": "3.1.1",
      "@types/merge-images": "^1.2.0",
      "@types/node": "15.6.0",
      "@typescript-eslint/eslint-plugin": "4.24.0",
      "@typescript-eslint/parser": "4.24.0",
      "cross-env": "7.0.3",
      "dotenv": "10.0.0",
      "eslint": "7.27.0",
      "eslint-config-prettier": "8.3.0",
      "eslint-plugin-prettier": "3.4.0",
      "pre-commit": "1.2.2",
      "prettier": "2.3.0",
      "ts-node-dev": "1.1.6",
      "typescript": "4.2.4"
   }
}
